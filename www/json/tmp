    function press_key(e) {
        if (((document.all) ? e.keyCode : e.which) == "13") {
            BuscarGeneral();
        }
    };
    function BuscarGeneral() {
        var strQUERY;
        var strTipoBusqueda;
        strQUERY = document.getElementById("frase_busqueda").value;
        window.location = "/Buscador/?query=" + Url.encode(strQUERY.toLowerCase());
    }
</script>

